  "type": "commonjs",
  "engines": {
    "node": ">=22.22.0"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "typeorm": "ts-node -r tsconfig-paths/register --transpile-only ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm migration:generate migrations/$npm_config_name -- -d src/core/persistence/database/relational/database.source.ts",
    "migration:generate-mts": "npm run migration:generate && npx renamer --find '/(\\d{13}-)?($npm_config_name)\\.ts$/' --replace '$1$2.mts' migrations/*.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/core/persistence/database/relational/database.source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/core/persistence/database/relational/database.source.ts",
    "dev": "nest start --watch src/main.ts",
    "dev:fast": "bun run --watch src/main.ts",
    "build:fast": "bun build --target=bun -e @nestjs/websockets/socket-module -e ts-morph -e amqplib -e amqp-connection-manager -e nats -e @apollo/subgraph -e mqtt -e @nestjs/sequelize -e @mikro-orm/core -e @nestjs/typeorm -e @nestjs/mongoose -e class-transformer/storage -e kafkajs -e @nestjs/microservices   --outdir dist src/main.ts ",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pin": "ts-node src/utils/scripts/pin-cli/pin-cli.script.ts"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
